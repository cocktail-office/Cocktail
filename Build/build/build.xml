    <target name="dist">
        <!-- do dist target on known frameworks -->
        <antcall target="all.run" >
            <param name="dist.base" value="${dist.base}" />
            <param name="project.version" value="${project.version}" />
            <param name="dist.dir" value="${dist.base}/cocktailr-${project.version}"/>
            <param name="build.action" value="dist" />
        </antcall>

        <!-- copy the whole source to src -->
        <copy todir="${dist.base}/Cocktail-src-${project.version}"
                includeEmptyDirs="false" preservelastmodified="true">
            <fileset dir=".">
                <include name="**/*" />
                <exclude name="**/null*" />
                <exclude name="**/.git/**" />
                <exclude name="**/.svn/*" />
             	<exclude name="**/dist/**"/>
             	<exclude name="**/bin/**"/>
             	<exclude name="**/build/**"/>
             	<include name="**/Build/build/**"/>
             	<exclude name="**/target/**"/>
            </fileset>
        </copy>

        <tar tarfile="${dist.base}/Cocktail-src-${project.version}.tar" longfile="gnu">
            <tarfileset dir="${dist.base}/Cocktail-src-${project.version}">
            </tarfileset>
        </tar>

        <gzip src="${dist.base}/Cocktail-src-${project.version}.tar"
